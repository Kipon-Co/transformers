{
  "name": "json-transformer",
  "version": "1.0.0",
  "description": "Transform columnar SQL results into complex nested JSON objects with custom transformations",
  "author": "Kipon",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint src tests --ext .ts",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "sql",
    "json",
    "transform",
    "nested",
    "object",
    "mapper",
    "typescript"
  ],
  "dependencies": {
    "es-toolkit": "~1.32.0"
  },
  "devDependencies": {
    "@types/jest": "~29.5.14",
    "@types/node": "~22.13.5",
    "@typescript-eslint/eslint-plugin": "~8.25.0",
    "@typescript-eslint/parser": "~8.25.0",
    "eslint": "~9.21.0",
    "jest": "~29.7.0",
    "prettier": "~3.5.2",
    "ts-jest": "~29.2.6",
    "typescript": "~5.7.3"
  }
}
